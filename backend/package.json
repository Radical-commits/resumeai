{
  "name": "resumeai-backend",
  "version": "1.0.0",
  "description": "Backend API for ResumeAI - AI-powered resume website",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run copy-data",
    "copy-data": "mkdir -p dist/data && cp -r src/data/* dist/data/",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "express",
    "typescript",
    "gemini",
    "ai"
  ],
  "author": "Alexander Kirillov",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.16",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "openai": "^6.16.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.1"
  }
}
